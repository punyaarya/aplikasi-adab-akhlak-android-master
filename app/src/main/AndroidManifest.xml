<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.e.aplikasiadabakhlak">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cultures"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cultures"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity"/>
            <activity android:name=".LoginActivity"/>
            <activity android:name=".TambahActivity"/>
            <activity android:name=".AkunActivity"/>
            <activity android:name=".TampilActivity"/>
            <activity android:name=".EditActivity"/>
            <activity android:name=".AdabAkhlakActivity"/>
            <activity android:name=".Splash"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>